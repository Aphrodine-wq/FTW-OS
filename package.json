{
  "name": "ftwos",
  "version": "1.2.1",
  "description": "Full System for Dev Companies",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on tcp:5173 && tsc -p electron/tsconfig.json && electron .",
    "clean": "rimraf dist-electron dist_build dist_final",
    "build": "node scripts/deploy.js",
    "build:core": "npm run clean && tsc -p electron/tsconfig.json && vite build && electron-builder --config .build-config/electron-builder.yml",
    "build:react": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "deploy": "node scripts/deploy.js",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "keywords": [
    "invoice",
    "generator",
    "electron",
    "react"
  ],
  "author": "FTWOS Team",
  "license": "Private",
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/supabase-js": "^2.90.1",
    "@types/lodash": "^4.17.21",
    "@types/react-grid-layout": "^1.3.6",
    "chokidar": "^3.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^8.5.0",
    "electron-updater": "^6.7.3",
    "framer-motion": "^12.24.12",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "papaparse": "^5.5.3",
    "postcss": "^8.4.35",
    "puppeteer": "^22.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^2.2.2",
    "recharts": "^3.6.0",
    "sql.js": "^1.13.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.1",
    "twilio": "^5.11.2",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/sql.js": "^1.4.9",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "playwright": "^1.42.0",
    "rimraf": "^6.1.2",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "wait-on": "^7.2.0"
  }
}
