    "dompurify": "^3.3.1",
    "electron-packager": "^17.1.2",
    "electron-updater": "^6.7.3",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.24.12",
    "fs-extra": "^11.3.3",
