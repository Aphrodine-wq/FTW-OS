{
  "name": "ftwos",
  "version": "1.0.14",
  "description": "Full System for Dev Companies - Now with Neural Flow AI",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:auto": "node scripts/dev-launcher.js",
    "dev:watch": "node scripts/dev-launcher.js",
    "start": "electron .",
    "dev:react": "node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js",
    "dev:electron": "wait-on tcp:5173 && tsc -p electron/tsconfig.json && electron . --dev",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist-electron dist_build dist_installer dist_final dist_ftwos dist_out dist_release_* dist_v* releases/v* FTW-OS-Setup.exe",
    "build": "node scripts/release.js && node scripts/create-setup.js",
    "build:installer": "node scripts/build-installer.js",
    "create-setup": "node scripts/create-setup.js",
    "build:core": "npm run clean && tsc -p electron/tsconfig.json && vite build && electron-builder --config .build-config/electron-builder.yml",
    "build:react": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "deploy": "node scripts/release.js",
    "preview": "vite preview",
    "analyze": "node scripts/analyze-bundle.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "keywords": [
    "invoice",
    "generator",
    "electron",
    "react"
  ],
  "author": "FTWOS Team",
  "license": "Private",
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@monaco-editor/react": "^4.7.0",
    "@octokit/rest": "^22.0.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.16",
    "@types/dompurify": "^3.0.5",
    "@types/file-saver": "^2.0.7",
